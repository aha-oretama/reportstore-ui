{
  "name": "testerve-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "_docker-start:test": "if test \"$CI\" != \"true\"; then docker-compose -p test -f docker/test-compose.yaml up --force-recreate -d; ./docker/bin/wait-for-postgres.sh test; fi",
    "predev": "docker-compose -p development -f docker/dev-compose.yaml up --force-recreate -d; ./docker/bin/wait-for-postgres.sh development",
    "db-migrate:development": "sequelize db:migrate --env development",
    "db-force-migrate:development": "db:migrate:undo:all --env development; sequelize db:migrate --env development",
    "pretest": "npm run _docker-start:test; sequelize db:migrate:undo:all --env test; sequelize db:migrate --env test",
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "bin": {
    "upload": "bin/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "npm run lint",
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.16.0",
    "@octokit/rest": "^18.0.12",
    "auth0": "^2.31.1",
    "autoprefixer": "^10.2.3",
    "bent": "^7.3.12",
    "commander": "^6.2.1",
    "date-fns": "^2.16.1",
    "fast-xml-parser": "^3.17.6",
    "luxon": "^1.25.0",
    "next": "^10.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "postcss": "^8.2.4",
    "react": "16.13.1",
    "react-accessible-accordion": "^3.3.4",
    "react-dom": "16.13.1",
    "sequelize": "^6.3.5",
    "swr": "^0.3.11",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@octokit/types": "^6.2.1",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/auth0": "^2.20.13",
    "@types/jest": "^26.0.19",
    "@types/luxon": "^1.25.1",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.1.3"
  }
}
